{
  "manifest_version": 3,
  "name": "BerlinTech Kommo",
  "version": "1.0.19.0",
  "version_name": "1.0.19",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "tabs"
  ],
  "content_scripts": [
    {
      "js": [
        "assets/content.ts-loader.js"
      ],
      "matches": [
        "https://subscriptionberlintechai.kommo.com/*"
      ],
      "css": [
        "assets/content.css"
      ]
    }
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "web_accessible_resources": [
    {
      "matches": [
        "https://subscriptionberlintechai.kommo.com/*"
      ],
      "resources": [
        "assets/utils.js",
        "assets/content.ts.js"
      ],
      "use_dynamic_url": true
    }
  ]
}
